{
	"extends": "../tsconfig.json",
	"compilerOptions": {
		"baseUrl": "./",
		"module": "ESNext",
		"target": "ES2020",
		"moduleResolution": "Node",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	},
	"include": ["**/*.ts", "**/*.tsx"],
	"exclude": ["node_modules", "dist"]
}
